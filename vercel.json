{
    "version": 2,
    "builds": [
        {
            "src": "server/package.json",
            "use": "@vercel/node"
        },
        {
            "src": "client/package.json",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server/src/server.ts"
        }
    ]
}